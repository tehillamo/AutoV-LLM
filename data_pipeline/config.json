{
    "input_path": "../ressources",
    "output_path": "../output/result.csv",
    "cache_path": "../cache",

    "transcription_model": "large",
    "behavioral_columns": [ "stimulus", "response", "rt", "pair" ],
    "reduction_algorithm": [ "PCA", "TSNE", "both"],
    "dimension": 2,
    "reduction_per_participant": false,

    "transcribe_text": true,
    "word_cloud": false,
    "ignore_words_in_word_cloud": ["Thank", "Thank you", "Thanks"],
    "text_classification": false,
    "text_classification_threshold": 0,
    "calculate_text_embeddings": true,
    "dimensionality_reduction": false,
    "text_classes": ["absolutely uncertain", "very uncertain", "somewhat uncertain", "a little uncertain", "a little certain", "somewhat certain", "very certain", "absolutely certain"],
    "keywords": false,
    "top_n_keywords": 2,

    "summarize": false,
    "max_length_summary": 130,
    "min_length_summary": 30,

    "use_openai_prompting": false,
    "use_openai_embeddings": false,
    "developer_prompt": "Tell me if this is positive or negative sentiment.",
    "openai_model": "gpt-4o-mini",
    "openai_embeddings_model": "text-embedding-3-small",
    "openai_api_key": "<API_KEY_PLACEHOLDER>",

    "zero_shot_text_finetuned_model": null,
    "bert_finetuned_model": null
}